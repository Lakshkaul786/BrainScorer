{
    "manifest_version": 2,
    "name": "BrainScore",
    "description": "catalog score just for you",
    "version": "3.0",
    "icons" :{
        "32": "/images/Logo32.png"
    },
    "browser_action": {
        "default_icon":{
            "32": "/images/Logo32.png"
        },
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "host_permissions": [
        "http://*/*",
        "http://*/*"
    ],
    "permissions":[
        "scripting"
    ]
}
