{
    "manifest_version": 2,
    "name": "Ecommerce Catalog Scorer",
    "version": "1.0",
    "description": "Evaluate ecommerce catalogs on design, usability, and pricing.",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.jpg",
        "48": "icons/icon48.jpg",
        "128": "icons/icon128.jpg"
      }
    },
    "icons": {
        "16": "icons/icon16.jpg",
        "48": "icons/icon48.jpg",
      "128": "icons/icon128.jpg"
    },
    "permissions": ["tabs"]
  }
  